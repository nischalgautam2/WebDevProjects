#include <bits/stdc++.h>
using namespace std;

bool isPrime(int n){
    bool ans = true;
    for (int i=2;i<n;i++){
        if(n%i==0){
            ans = false;
        }
    }
    return ans;
}

int main (){
    int a;
    cout<<"Enter number a : ";
    cin>>a;
    if (isPrime(a)){
        cout<<a<<" is a prime number"<<endl;
    }
    else {
        cout<<a<<" is not a prime number"<<endl;
    }
    return 0;


}
